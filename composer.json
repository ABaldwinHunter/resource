{
  "name": "apparat/resource",
  "description": "Apparat resource abstraction layer",
  "homepage": "https://apparat.tools",
  "minimum-stability": "RC",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net",
      "homepage": "https://jkphl.is",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "apparat/kernel": "@dev",
    "league/commonmark": "^0.12.0",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    },
    "files": [
      "src/Resource/Module.php",
      "src/Resource/Autorun.php"
    ]
  }
}
